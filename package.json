{
  "name": "@turbot/log",
  "author": "Turbot HQ, Inc",
  "description": "Turbot Log Library",
  "version": "5.1.0",
  "license": "Apache-2.0",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/turbot/log.git"
  },
  "scripts": {
    "coverage": "npx nyc npm test",
    "test": "npx mocha"
