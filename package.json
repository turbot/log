    "@turbot/utils": "5.1.0",
    "serialize-error": "6.0.0"
  },
  "devDependencies": {
    "chai": "4.2.0",
    "eslint": "6.8.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-prettier": "3.1.3",
    "eslint-plugin-you-dont-need-lodash-underscore": "6.10.0",
    "mocha": "7.1.2",
    "nyc": "15.0.1",
    "mocha-sinon": "2.1.2",
    "prettier": "2.0.5",
    "test-console": "1.1.0"
  },
  "eslintConfig": {
    "extends": [
      "plugin:prettier/recommended",
      "eslint:recommended",
      "plugin:you-dont-need-lodash-underscore/compatible"
    ],
    "env": {
      "es6": true,
      "node": true,
      "mocha": true
    },
    "parserOptions": {
      "ecmaVersion": 10
    },
    "rules": {
      "no-console": "off",
      "you-dont-need-lodash-underscore/is-nil": "off"
    }
  },
  "prettier": {
    "printWidth": 120
  }
